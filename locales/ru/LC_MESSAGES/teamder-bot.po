#: tgbot/handlers/admin.py:19
msgid ""
"Hello {user_name}!\n"
"You in the admin panel"
msgstr ""
"Привет {user_name}!\n"
"Ты в панели администратора"

#: tgbot/handlers/admin.py:40
msgid ""
"{num}. {user_id} <a href='tg://user?id={user_id}'><b>{fullname}</b></a> "
"{username}[{date}]\n"
msgstr ""
"{num}. <code>{user_id}</code> <a href='tg://user?id={user_id}'><b>{fullname}"
"</b></a> {username}[{date}]\n"

#: tgbot/handlers/admin.py:66
msgid "No users was found"
msgstr "Ни одного пользователя не было найдено"

#: tgbot/handlers/admin.py:80
msgid "{num}. <a href='tg://user?id={user_id}'><b>{user_id}</b></a> [{date}]\n"
msgstr ""
"{num}. <a href='tg://user?id={user_id}'><b>{user_id}</b></a> [{date}]\n"

#: tgbot/handlers/admin.py:103
msgid "No admins was added"
msgstr "Ни одного администратора не было добавлено"

#: tgbot/handlers/admin.py:114
msgid "Send user id or forward message from user who will be an admin"
msgstr ""
"Оправьте айди пользователя, который станет админом, или перешлите его "
"сообщение мне"

#: tgbot/handlers/admin.py:133 tgbot/handlers/admin.py:224
msgid ""
"User id is invalid! Forward to me any message from this user, or send me his "
"id. You can find it, for example, from the bot @my_id_bot"
msgstr ""
"Айди пользователя неверно! Перешлите мне любое его сообщение или отправьте "
"мне его айди. Узнать его вы можете например у бота @my_id_bot"

#: tgbot/handlers/admin.py:145 tgbot/handlers/admin.py:236
msgid ""
"Forwarded message was not sent by user! Forward to me any message from this "
"user, or send me his id. You can find it, for example, from the bot "
"@my_id_bot"
msgstr ""
"Пересланное сообщение было отправлено не пользователем! Перешлите мне любое "
"его сообщение или отправьте мне его айди. Узнать его вы можете например у "
"бота @my_id_bot"

#: tgbot/handlers/admin.py:157
msgid "Are you sure you want to add user {user_id} as an admin?"
msgstr ""
"Вы уверены, что хотите добавить пользователя <code>{user_id}</code> как "
"адмнистратора?"

#: tgbot/handlers/admin.py:168
msgid "User is already an admin"
msgstr "Этот пользователь уже администратор"

#: tgbot/handlers/admin.py:189
msgid "User {user_id} was added as an admin!"
msgstr "Пользователь <code>{user_Id}</code> был добавлен в адмнистраторы!"

#: tgbot/handlers/admin.py:205
msgid "Send user id or forward message from user who will removed from admins"
msgstr ""
"Оправьте айди пользователя, которого нужно удалить из адмнистраторов, или "
"перешлите его сообщение мне"

#: tgbot/handlers/admin.py:247
msgid "Are you sure you want to remove user {user_id} from admins?"
msgstr ""
"Вы уверены, что хотите удалить пользователя <code>{user_id}</code> из "
"администраторов?"

#: tgbot/handlers/admin.py:259
msgid "This user is not an admin"
msgstr "Этот пользователь не администратор"

#: tgbot/handlers/admin.py:280
msgid "User {user_id} was removed from admins!"
msgstr "Пользователь <code>{user_id}</code> был удален из администраторов!"

#: tgbot/handlers/admin.py:297 tgbot/handlers/reply/admin_kb.py:10
msgid "List users"
msgstr "Список пользователей"

#: tgbot/handlers/admin.py:303 tgbot/handlers/reply/admin_kb.py:13
msgid "List admins"
msgstr "Список администраторов"

#: tgbot/handlers/admin.py:309 tgbot/handlers/reply/admin_kb.py:16
msgid "Add admin"
msgstr "Добавить администратора"

#: tgbot/handlers/admin.py:323 tgbot/handlers/reply/admin_kb.py:17
msgid "Delete admin"
msgstr "Удалить администратора"

#: tgbot/handlers/user.py:16
msgid "Action was canceled"
msgstr "Действие было отменено"

#: tgbot/handlers/user.py:29
msgid "Hello, user!"
msgstr "Привет, пользователь!"

#: tgbot/handlers/user.py:40
msgid "Your id is {user_id}"
msgstr "Твой айди <code>{user_id}</code>"

#: tgbot/handlers/inline/example_kb.py:16
msgid "Show my id"
msgstr "Показать мой айди"

#: tgbot/handlers/inline/admin/admin_add_conf_kb.py:15
#: tgbot/handlers/inline/admin/admin_delete_conf_kb.py:15
msgid "Yes"
msgstr "Да"

#: tgbot/handlers/inline/admin/admin_add_conf_kb.py:21
#: tgbot/handlers/inline/admin/admin_delete_conf_kb.py:21
msgid "No"
msgstr "Нет"
